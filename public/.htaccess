<IfModule mod_rewrite.c>


  RewriteEngine On
  RewriteBase /
  RewriteRule ^index\.html$ - [L]
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteCond %{REQUEST_FILENAME} !-l
  RewriteRule . /index.html [L]
  
  RewriteCond %{HTTP_HOST} ^www.samurai-social-network.netlify.app$ [NC] 
  RewriteRule ^(.*)$ https://samurai-social-network.netlify.app/$1 [R=301,L]


</IfModule>